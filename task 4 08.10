print('a*x**2 +b*x + c=0')
a = input('Введите a:')
b = input('Введите b:')
c = input('Введите c:')
a = float(a)
b = float(b)
c = float(c)
disc = b**2 - 4*a*c
print('Дискриминант' + str(disc))
if disc < 0:
    print('корней нет')
elif disc == 0:
    x = -b / (2 * a)
    print('x = ' +str(x))
else:
    x1 = (-b + disc ** 0,5) / (2 * a)
    x2 = (-b - disc ** 0,5) / (2 * a)
    print('x1 = ' + str(x1))
    print('x2 = ' + str(x2))
